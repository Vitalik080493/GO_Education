import (
    // Делает пакет fmt (формат) доступным для использования
    "fmt"
)

func main() {
  fmt.Println("Hello") // вывеси строку с посл. перех. на новую.
  fmt.Print ("Hello") // выв. стр. без посл перех. на нов. стр.
  fmt.Printf("Мой вес на поверхности %v равен %v килограммам.\n", "Земли", 55) // выв. стр. с подст. Аргументов
  fmt.Printf("Type %T for %v\n", year, year) // Type int for 2018
}
